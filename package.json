{
  "name": "ems",
  "version": "1.0.0",
  "private": true,
  "description": "EMS - Backend and Frontend monorepo",
  "scripts": {
    "dev": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "dev:be": "cd EMS-BE && npm run dev",
    "dev:fe": "cd EMS-FE && npm start",
    "start:be": "cd EMS-BE && npm start",
    "start:fe": "cd EMS-FE && npm start",
    "install:all": "npm install && cd EMS-BE && npm install && cd ../EMS-FE && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
